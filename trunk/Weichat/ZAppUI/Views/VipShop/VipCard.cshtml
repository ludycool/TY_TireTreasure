@model System.Data.DataRow

@{
    ViewBag.Title = "我的会员卡";
}

<html lang="zh-CN">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
<!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
<title>3W联盟</title>

<!-- Bootstrap -->
<link href="../../Content/css/member_card.css" rel="stylesheet">

<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
<script src="//cdn.bootcss.com/html5shiv/3.7.2/html5shiv.min.js"></script>
<script src="//cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
<![endif]-->
     @section Jsprepare{

         <script type="text/javascript">
           
          jQuery(document).ready(function () {
            
          })
            </script>
         }
</head>
<body>
<div class="container">
<!-- top nav -->
<div class="row">
  <div class="col-xs-12 top-parent-box">
    <a class="col-xs-6 top-child-box" href="/VipShop"><div class=""><img class="center-block" src="../../Content/images/left.png" alt=""></div></a>
    <!-- <a class="col-xs-6 top-child-box" href="#"><div class="box-right"><img class="center-block" src="images/plus.png" alt=""></div></a> -->
  </div>
</div>
<!-- main -->

<div class="row main">
	<div class="col-xs-12">
		<ul class="list-group">
		  <li class="list-group-item">
				<h4 class="list-group-item-heading">@Model["Nickname"] 会员卡</h4>
				<p class="list-group-item-text">余额：@Model["UMoney"] 元</p>
				<p class="list-group-item-text">编号：@Model["CarNo"]</p>
				<p class="list-group-item-text">开卡日期：@Model["StarTime"]</p>
				<p class="list-group-item-text">会员期限：@Model["EndTime"]</p>
				<p class="list-group-item-text">会员特权：@Model["Details"]</p>
		  </li>
		</ul>
	</div><!-- /.col-xs-12  -->


</div><!-- /.row  -->


</div><!-- /.container  -->
</body>
</html>
